# your Home work
pipeline {
    agent any
    parameters {
        string(name: 'NAME', defaultValue: 'Alice', description: 'Enter your name')
       string(name: 'Ages', defaultValue: '25', description: 'Enter your age')
    }
    stages {
        stage('Example') {
            steps {
                echo "Hello from Slave ${params.NAME} of ${params.Ages}"
                
            }
        }
    }
}
